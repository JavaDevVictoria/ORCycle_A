            android:id="@+id/TextViewMapFancyStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="4dp"
            android:text="September 27, 2014 11:02 PM"
            android:textColor="#E6DC8F"
            android:textSize="15sp" >
        </TextView>
        
        <TextView
            android:id="@+id/TextViewMapPurpose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="4dp"
            android:layout_below="@id/TextViewMapFancyStart"
            android:text="Trip Purpose:"
            android:textColor="#ffffff"
            android:textSize="18sp"
            android:textStyle="bold" >
        </TextView>

	    <TextView
	        android:id="@+id/TextViewMapInfo"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="4dp"
	        android:layout_marginTop="4dp"
	        android:layout_below="@id/TextViewMapPurpose"
	        android:paddingBottom="8dp"
	        android:text="Trip info: The quick brown fox jumps over the lazy dog"
	        android:textColor="#E6DC8F" >

	    </TextView>
    
	    </RelativeLayout>

