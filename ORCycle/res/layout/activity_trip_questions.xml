<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ScrollView01"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context=".UserInfo$DummySectionFragment" >

        <TextView
            android:id="@+id/tvTripFrequency"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q19_TripFrequency"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrTripFrequency"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvTripFrequency"
            android:layout_marginTop="20dp"
            android:entries="@array/qa_19_routeFrequency" />

        <TextView
            android:id="@+id/tvTripPurpose"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrTripFrequency"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q20_TripPurpose"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrTripPurpose"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvTripPurpose"
            android:layout_marginTop="20dp"
            android:entries="@array/qa_20_tripPurpose" />

        <TextView
            android:id="@+id/tvRouteChoice"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrTripPurpose"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q21_routePreferences"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <edu.pdx.cecs.orcycle.MultiSelectionSpinner
            android:id="@+id/spnrRouteChoice"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvRouteChoice"
            android:layout_marginTop="20dp" />

        <TextView
            android:id="@+id/tvTripComfort"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrRouteChoice"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q22_routeComfort"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrTripComfort"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvTripComfort"
            android:layout_marginTop="20dp"
            android:entries="@array/qa_22_routeComfort" />

        <TextView
            android:id="@+id/tvRouteSafety"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrTripComfort"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q23_RouteSafety"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrRouteSafety"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvRouteSafety"
            android:layout_marginTop="20dp"
            android:entries="@array/qa_23_RouteSafety" />

        <TextView
            android:id="@+id/tvRidePassengers"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrRouteSafety"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q24_ridePassengers"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <edu.pdx.cecs.orcycle.MultiSelectionSpinner
            android:id="@+id/spnrPassengers"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvRidePassengers"
            android:layout_marginTop="20dp" />

        <TextView
            android:id="@+id/tvBikeAccessories"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrPassengers"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q25_bikeAccessories"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <edu.pdx.cecs.orcycle.MultiSelectionSpinner
            android:id="@+id/spnrBikeAccessories"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvBikeAccessories"
            android:layout_marginTop="20dp" />

        <TextView
            android:id="@+id/tvRideConflict"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrBikeAccessories"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q26_rideConflict"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrRideConflict"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvRideConflict"
            android:layout_marginTop="20dp"
            android:entries="@array/qa_26_rideConflict" />

        <TextView
            android:id="@+id/tvRouteStressor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrRideConflict"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q27_routeStressors"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <edu.pdx.cecs.orcycle.MultiSelectionSpinner
            android:id="@+id/spnrRouteStressor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/tvRouteStressor"
            android:layout_marginTop="20dp" />

        <TextView
            android:id="@+id/tvRequiredTripQuestions"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrRouteStressor"
            android:layout_marginTop="15dp"
            android:text="@string/atq_requiredEntries"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        </RelativeLayout>
        
</ScrollView>