<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ScrollView01"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context=".UserInfo$DummySectionFragment" >

        <TextView
            android:id="@+id/tvRiderAbility"
            android:layout_width="135dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="5dp"
            android:text="@string/qa_16_riderAbility"
	        android:background="@drawable/lcdscreen"
            android:textAppearance="?android:attr/textAppearanceMedium" />

			<Spinner
            android:id="@+id/spnrRiderAbility"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvRiderAbility"
            android:entries="@array/qa_16_riderAbility" />

        <TextView
            android:id="@+id/tvRiderType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrRiderAbility"
            android:layout_marginTop="15dp"
            android:text="@string/q17_ridertype"
	        android:background="@drawable/lcdscreen"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrRiderType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvRiderType"
            android:entries="@array/qa_17_ridertype" />

        <TextView
            android:id="@+id/tvCycleFreq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrRiderType"
            android:layout_marginTop="15dp"
            android:text="@string/q14_cyclingFrequency"
	        android:background="@drawable/lcdscreen"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrCycleFrequency"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvCycleFreq"
            android:entries="@array/qa_14_cyclingFrequency" />

        <TextView
            android:id="@+id/textViewCycleWeather"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrCycleFrequency"
            android:layout_marginTop="15dp"
            android:text="@string/q15_cyclingWeather"
	        android:background="@drawable/lcdscreen"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrCycleWeather"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewCycleWeather"
            android:entries="@array/qa_15_cyclingWeather" />

        <TextView
            android:id="@+id/tvNumBikes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrCycleWeather"
            android:layout_marginTop="15dp"
            android:text="@string/q09_numBikes"
	        android:background="@drawable/lcdscreen"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrNumBikes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvNumBikes"
            android:entries="@array/qa_09_numBikes" />

        <TextView
            android:id="@+id/tvBikeTypes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrNumBikes"
            android:layout_marginTop="15dp"
            android:text="@string/q10_bikeTypes"
	        android:background="@drawable/lcdscreen"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <edu.pdx.cecs.orcycle.MultiSelectionSpinner
            android:id="@+id/spnrBikeTypes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvBikeTypes" />

        <TextView
            android:id="@+id/tvOccupation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrBikeTypes"
            android:layout_marginTop="15dp"
            android:text="@string/q05_occupation"
	        android:background="@drawable/lcdscreen"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrOccupation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvOccupation"
            android:entries="@array/qa_05_occupation" />

        <TextView
            android:id="@+id/tvAge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/spnrOccupation"
            android:layout_marginTop="15dp"
	        android:background="@drawable/lcdscreen"
            android:text="@string/q01_age"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <Spinner
            android:id="@+id/spnrAge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/tvAge"
            android:entries="@array/qa_01_age" />

        <TextView
            android:id="@+id/tvGender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/spnrAge"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q03_gender"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrGender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvGender"
            android:entries="@array/qa_03_gender" />
            
        <TextView
            android:id="@+id/tvVehicles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrGender"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q08_hhVehicles"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <Spinner
            android:id="@+id/spnrVehicles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvVehicles"
            android:entries="@array/qa_08_hhVehicles" />
            
        <TextView
            android:id="@+id/tvWorkers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrVehicles"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q07_hhWorkers"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
        <Spinner
            android:id="@+id/spnrWorkers"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvWorkers"
            android:entries="@array/qa_07_hhWorkers" />
            
        <TextView
            android:id="@+id/tvEthnicity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrWorkers"
            android:layout_marginTop="15dp"
	        android:background="@drawable/lcdscreen"
            android:text="@string/q04_ethnicity"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrEthnicity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvEthnicity"
            android:entries="@array/qa_04_ethnicity" />
        
        <TextView
            android:id="@+id/tvIncome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/spnrEthnicity"
            android:layout_marginTop="15dp"
            android:background="@drawable/lcdscreen"
            android:text="@string/q06_income"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Spinner
            android:id="@+id/spnrIncome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/tvIncome"
            android:entries="@array/qa_06_income" />

        <TextView
            android:id="@+id/tvEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/spnrIncome"
            android:layout_marginTop="15dp"
            android:text="@string/q02_email"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/editEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@id/tvEmail"
            android:layout_alignBottom="@id/tvEmail"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/tvEmail"
            android:ems="20"
            android:hint="@string/atq_email_hint"
            android:inputType="textEmailAddress" />

        </RelativeLayout>

</ScrollView>